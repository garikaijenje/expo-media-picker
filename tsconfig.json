{
//   "extends": "expo/tsconfig.base",
  "include": ["src", "types"],
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "jsx": "react-native",
    "lib": ["DOM", "ESNext"],
    "moduleResolution": "node",
    // "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "strict": true,
    "sourceMap": true,
    "rootDir": "./src",
    "outDir": "dist",
  },
  "exclude": ["node_modules", "dist", "example"]
}
